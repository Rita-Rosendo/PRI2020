<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT ARQSITS (ARQELEM)+ >
<!ELEMENT ARQELEM (TIPO,IDENTI,IMAGEM?, DESCRI, CRONO?, LUGAR, FREGUE, CONCEL, CODADM, LATITU, ALTITU, ACESSO, QUADRO, TRAARQ?, DESARQ, INTERP, INTERE?, DEPOSI, BIBLIO*, AUTOR, DATA) >

<!ELEMENT TIPO (#PCDATA) >
<!ATTLIST TIPO
    ASSUNTO CDATA #REQUIRED >

<!ELEMENT IDENTI (#PCDATA) >

<!ELEMENT IMAGEM (#PCDATA) >
<!ATTLIST IMAGEM
    NOME CDATA #REQUIRED >
    
<!ELEMENT DESCRI (LIGA)>
<!ELEMENT LIGA (#PCDATA) >
<!ATTLIST LIGA
    TERMO CDATA #REQUIRED >
<!ELEMENT CRONO (#PCDATA) >
<!ELEMENT LUGAR (#PCDATA) >
<!ELEMENT FREGUE (#PCDATA) >
<!ELEMENT CONCEL (#PCDATA) >
<!ELEMENT CODADM (#PCDATA) >
